<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <!-- Stylesheets -->
    <link rel="icon" href="/favicon.ico" type="image/x-icon">
    
    <link href="~/css/main.7412d994a62944a2acaf.css" rel="stylesheet" />
    
    <!-- End Stylesheets -->
    <style>
        input.input-validation-error {
            border-color: red;
        }
    </style>
</head>
<body>
    <nav class="navbar" role="navigation">
        <div class="navbar-menu">
            <div class="navbar-start">
            <a class="navbar-item"  asp-controller="Home" asp-action="Index">Home</a>
            <a class="navbar-item" asp-controller="Users" asp-action="Index">Users</a>
            <a class="navbar-item" asp-controller="Groups" asp-action="Index">Groups</a>
            <a class="navbar-item" asp-controller="Gifts" asp-action="Index">Gifts</a>
            </div>
        </div>
    </nav>
    <div>
        @RenderBody()
    </div>
    <!-- Scripts -->
    
    <script src="~/js/main.7412d994a62944a2acaf.js"></script>
    
    <!-- End Scripts -->
    @RenderSection("scripts", required: false)
</body>
</html>
